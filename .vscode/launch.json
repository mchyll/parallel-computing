    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug ass. 5 pthread",
            "type": "cppdbg",
            "request": "launch",
            "preLaunchTask": "Build ass. 5 pthread",
            "program": "${workspaceFolder}/assignment5/pthread/mandel/mandel",
            "args": ["-ooutput/mandel.bmp", "-r2048", "-x0.5", "-y0.5", "-s1.0", "-p4", "-c1", "-i512", "-m"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/assignment5/pthread",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Debug ass. 5 openmp",
            "type": "cppdbg",
            "request": "launch",
            "preLaunchTask": "Build ass. 5 openmp",
            "program": "${workspaceFolder}/assignment5/openmp/main",
            "args": ["b", "t"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/assignment5/openmp",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}